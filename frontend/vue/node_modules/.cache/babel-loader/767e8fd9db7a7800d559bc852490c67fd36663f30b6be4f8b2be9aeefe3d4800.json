  // 参考\n      this.$request.get('/diskFiles/count', { params: { days: this.days } }).then(res => {\n        lineOption.xAxis.data = res.data?.map(v => v.date) || []\n        lineOption.series[0].data = res.data.map(v => v.count) || []\n        lineChart.setOption(lineOption)\n      })\n    }\n  }\n}\n</script>\n"],"mappings":"AAuCA,YAAAA,OAAA;AACA,MAAAC,UAAA;EACAC,KAAA;IACAC,IAAA;IACAC,IAAA;EACA;EACAC,OAAA;IACAC,OAAA;EACA;EACAC,MAAA;IACAH,IAAA;EACA;EACAI,KAAA;IACAC,IAAA;IACAC,IAAA;EACA;EACAC,KAAA;IACAF,IAAA;EACA;EACAG,MAAA,GACA;IACAF,IAAA;IACAD,IAAA;IACAI,MAAA;EACA;AAEA;AAEA;EACAC,IAAA;EACAJ,KAAA;IACA;MACAK,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,OAAA;MACAC,IAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,QAAA,CAAAC,GAAA,sBAAAC,IAAA,CAAAC,GAAA;MACA,KAAAN,OAAA,GAAAM,GAAA,CAAAhB,IAAA;IACA;EACA;EACAiB,QAAA;IACA,KAAAC,QAAA;EACA;EACAC,OAAA;IACAD,SAAA;MACA;MACA,IAAAE,QAAA,GAAAC,QAAA,CAAAC,cAAA;MACA,IAAAC,SAAA,GAAAjC,OAAA,CAAAkC,IAAA,CAAAJ,QAAA;MACA;MACA,KAAAP,QAAA,CAAAC,GAAA;QAAAW,MAAA;UAAAd,IAAA,OAAAA;QAAA;MAAA,GAAAI,IAAA,CAAAC,GAAA;QACAzB,UAAA,CAAAO,KAAA,CAAAE,IAAA,GAAAgB,GAAA,CAAAhB,IAAA,EAAA0B,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,IAAA;QACArC,UAAA,CAAAW,MAAA,IAAAF,IAAA,GAAAgB,GAAA,CAAAhB,IAAA,CAAA0B,GAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAE,KAAA;QACAN,SAAA,CAAAO,SAAA,CAAAvC,UAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
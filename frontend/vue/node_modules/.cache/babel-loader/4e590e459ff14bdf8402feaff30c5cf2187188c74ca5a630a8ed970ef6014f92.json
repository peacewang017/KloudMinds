{"ast":null,"code":"export default {\n  name: \"Login\",\n  data() {\n    return {\n      dialogVisible: true,\n      form: {\n        role: 'ADMIN'\n      },\n      rules: {\n        username: [{\n          required: true,\n          message: '请输入账号',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }]\n      },\n      imgs: [require('@/assets/imgs/1.jpg'), require('@/assets/imgs/2.jpg'), require('@/assets/imgs/3.jpg')],\n      slideVerifyShow: false\n    };\n  },\n  created() {},\n  methods: {\n    onSuccess() {\n      this.$request.post('/login', this.form).then(res => {\n        if (res.code === '200') {\n          localStorage.setItem(\"xm-user\", JSON.stringify(res.data)); // 存储用户数据\n          this.$message.success('登录成功');\n          setTimeout(() => {\n            if (res.data.role === 'ADMIN') {\n              location.href = '/home';\n            } else {\n              location.href = '/front/home?category=all';\n            }\n          }, 500);\n        } else {\n          this.$message.error(res.msg);\n          this.slideVerifyShow = false;\n        }\n      });\n    },\n    login() {\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          this.slideVerifyShow = true;\n          // 验证通过\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","dialogVisible","form","role","rules","username","required","message","trigger","password","imgs","require","slideVerifyShow","created","methods","onSuccess","$request","post","then","res","code","localStorage","setItem","JSON","stringify","$message","success","setTimeout","location","href","error","msg","login","$refs","validate","valid"],"sources":["src/views/Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div style=\"height: 60px; position: fixed; top: 0; display: flex; align-items: center; padding-left: 20px\">\r\n      <img src=\"@/assets/imgs/logo.png\" alt=\"\" style=\"width: 50px;transform: scale(1.5);\">\r\n      <span style=\"font-family: 'Futura PT', sans-serif;color: #81a5f9; font-size: 24px; font-weight: bold; margin-left: 50px;transform: scale(1.5); text-shadow: 1px 2px 5px black;\">KloudMinds</span>\r\n    </div>\r\n\r\n    <div style=\"flex: 1; display: flex; justify-content: center\">\r\n      <img src=\"@/assets/imgs/log.svg\" alt=\"\" style=\"width: 80%\">\r\n    </div>\r\n\r\n    <div style=\"flex: 1; display: flex; justify-content: center\">\r\n      <div style=\"width: 350px; position: relative; padding: 50px 30px; box-shadow: 0 0 10px rgba(0,0,0,.2); background-color: white; border-radius: 5px;\">\r\n        <div style=\"position: absolute; background-color: white; top: 100px; z-index: 999\" v-if=\"slideVerifyShow\">\r\n          <slide-verify :l=\"42\"\r\n                        :r=\"10\"\r\n                        :w=\"300\"\r\n                        :h=\"155\"\r\n                        :accuracy=\"5\"\r\n                        :imgs=\"imgs\"\r\n                        slider-text=\"向右滑动\"\r\n                        @success=\"onSuccess\"\r\n                        ref=\"slideVerifyRef\"\r\n          ></slide-verify>\r\n        </div>\r\n        <div style=\"text-align: center; font-size: 24px; font-weight: bold; margin-bottom: 30px; color: #333\">欢 迎 登 录</div>\r\n        <el-form :model=\"form\" :rules=\"rules\" ref=\"formRef\">\r\n          <el-form-item prop=\"username\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-user\" placeholder=\"请输入账号\" v-model=\"form.username\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"password\">\r\n            <el-input size=\"medium\" prefix-icon=\"el-icon-lock\" placeholder=\"请输入密码\" show-password  v-model=\"form.password\"></el-input>\r\n          </el-form-item>\r\n          <el-form-item prop=\"role\">\r\n            <el-select style=\"width: 100%\" size=\"medium\" v-model=\"form.role\">\r\n              <el-option value=\"ADMIN\" label=\"管理员\"></el-option>\r\n              <el-option value=\"USER\" label=\"用户\"></el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <div style=\"display: flex; align-items: center\">\r\n            <div style=\"flex: 1\"></div>\r\n            <div style=\"flex: 1; text-align: right\">\r\n              还没有账号？请 <a href=\"/register\">注册</a>\r\n            </div>\r\n          </div>\r\n        </el-form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Login\",\r\n  data() {\r\n    return {\r\n      dialogVisible: true,\r\n      form: { role: 'ADMIN' },\r\n      rules: {\r\n        username: [\r\n          { required: true, message: '请输入账号', trigger: 'blur' },\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'blur' },\r\n        ]\r\n      },\r\n      imgs: [\r\n        require('@/assets/imgs/1.jpg'),\r\n        require('@/assets/imgs/2.jpg'),\r\n        require('@/assets/imgs/3.jpg'),\r\n      ],\r\n      slideVerifyShow: false\r\n    }\r\n  },\r\n  created() {\r\n\r\n  },\r\n  methods: {\r\n    onSuccess() {\r\n      this.$request.post('/login', this.form).then(res => {\r\n        if (res.code === '200') {\r\n          localStorage.setItem(\"xm-user\", JSON.stringify(res.data))  // 存储用户数据\r\n          this.$message.success('登录成功')\r\n          setTimeout(() => {\r\n            if (res.data.role === 'ADMIN') {\r\n              location.href = '/home'\r\n            } else {\r\n              location.href = '/front/home?category=all'\r\n            }\r\n          }, 500)\r\n        } else {\r\n          this.$message.error(res.msg)\r\n          this.slideVerifyShow = false\r\n        }\r\n      })\r\n    },\r\n    login() {\r\n      this.$refs['formRef'].validate((valid) => {\r\n        if (valid) {\r\n          this.slideVerifyShow = true\r\n          // 验证通过\r\n\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.container {\r\n  height: 100vh;\r\n  overflow: hidden;\r\n  display: flex;\r\n  align-items: center;\r\n  background: linear-gradient(to bottom left, #81a5f9 300px, transparent 100px),\r\n  linear-gradient(to top left, #fec973, transparent);\r\n}\r\n\r\n.image-container img {\r\n  max-width: 100px; /* 根据需要调整图片大小 */\r\n}\r\n\r\na {\r\n  color: #2a60c9;\r\n}\r\n</style>\r\n"],"mappings":"AAoDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,aAAA;MACAC,IAAA;QAAAC,IAAA;MAAA;MACAC,KAAA;QACAC,QAAA,GACA;UAAAC,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,EACA;QACAC,QAAA,GACA;UAAAH,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA;MAEA;MACAE,IAAA,GACAC,OAAA,yBACAA,OAAA,yBACAA,OAAA,wBACA;MACAC,eAAA;IACA;EACA;EACAC,QAAA,GAEA;EACAC,OAAA;IACAC,UAAA;MACA,KAAAC,QAAA,CAAAC,IAAA,gBAAAf,IAAA,EAAAgB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACAC,YAAA,CAAAC,OAAA,YAAAC,IAAA,CAAAC,SAAA,CAAAL,GAAA,CAAAnB,IAAA;UACA,KAAAyB,QAAA,CAAAC,OAAA;UACAC,UAAA;YACA,IAAAR,GAAA,CAAAnB,IAAA,CAAAG,IAAA;cACAyB,QAAA,CAAAC,IAAA;YACA;cACAD,QAAA,CAAAC,IAAA;YACA;UACA;QACA;UACA,KAAAJ,QAAA,CAAAK,KAAA,CAAAX,GAAA,CAAAY,GAAA;UACA,KAAAnB,eAAA;QACA;MACA;IACA;IACAoB,MAAA;MACA,KAAAC,KAAA,YAAAC,QAAA,CAAAC,KAAA;QACA,IAAAA,KAAA;UACA,KAAAvB,eAAA;UACA;QAEA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}